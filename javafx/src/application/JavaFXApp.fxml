<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
            minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.JavaFXAppController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0"
                    AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <top>
                <MenuBar fx:id="menuBar" blendMode="LIGHTEN"
                         BorderPane.alignment="CENTER">
                    <menus>
                        <Menu mnemonicParsing="false" text="File">
                            <items>
                                <MenuItem mnemonicParsing="false"
                                          onAction="#command_LOAD_XML_FILE"
                                          text="Load XML"/>
                                <MenuItem mnemonicParsing="false"
                                          onAction="#command_SAVE_XML_FILE"
                                          text="Save XML"/>
                                <MenuItem mnemonicParsing="false"
                                          onAction="#command_EXIT" text="Exit"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Print">
                            <items>
                                <MenuItem mnemonicParsing="false"
                                          onAction="#printStocksOnTableView"
                                          text="Print Stocks"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Edit">
                            <items>
                                <MenuItem mnemonicParsing="false"
                                          text="Delete"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="View">
                            <items>
                                <MenuItem mnemonicParsing="false"
                                          onAction="#setFullScreen"
                                          text="FullScreen"/>
                                <MenuItem mnemonicParsing="false"
                                          onAction="#setColor"
                                          text="Custom Theme"/>
                            </items>
                        </Menu>
                        <Menu mnemonicParsing="false" text="Help">
                            <items>
                                <MenuItem mnemonicParsing="false" text="About"/>
                            </items>
                        </Menu>
                    </menus>
                </MenuBar>
            </top>
            <center>
                <BorderPane fx:id="borderPane" BorderPane.alignment="CENTER">
                    <center>
                        <StackPane BorderPane.alignment="CENTER">
                            <BorderPane.margin>
                                <Insets/>
                            </BorderPane.margin>
                        </StackPane>
                    </center>
                    <left>
                        <AnchorPane BorderPane.alignment="CENTER">
                            <children>
                                <BorderPane fx:id="leftBorderPane"
                                            AnchorPane.bottomAnchor="0.0"
                                            AnchorPane.leftAnchor="0.0"
                                            AnchorPane.rightAnchor="0.0"
                                            AnchorPane.topAnchor="0.0">
                                    <center>
                                        <VBox fx:id="menuVBox"
                                              alignment="TOP_CENTER">
                                            <children>
                                                <Button mnemonicParsing="false"
                                                        onAction="#scene1"
                                                        text="Scene 1"/>
                                                <Button fx:id="printStocksButton"
                                                        mnemonicParsing="false"
                                                        prefWidth="276.0"
                                                        text="Print Stocks"/>
                                            </children>
                                        </VBox>
                                    </center>
                                    <top>
                                        <AnchorPane
                                                BorderPane.alignment="CENTER">
                                            <BorderPane.margin>
                                                <Insets/>
                                            </BorderPane.margin>
                                            <children>
                                                <Label fx:id="rseLabel"
                                                       alignment="CENTER"
                                                       layoutX="59.0"
                                                       layoutY="2.0" text="RSE"
                                                       AnchorPane.bottomAnchor="0.0"
                                                       AnchorPane.leftAnchor="0.0"
                                                       AnchorPane.rightAnchor="0.0"
                                                       AnchorPane.topAnchor="0.0"/>
                                            </children>
                                        </AnchorPane>
                                    </top>
                                </BorderPane>
                            </children>
                        </AnchorPane>
                    </left>
                </BorderPane>
            </center>
            <bottom>
                <AnchorPane BorderPane.alignment="CENTER">
                    <children>
                        <BorderPane fx:id="bottomBorderPane"
                                    AnchorPane.bottomAnchor="0.0"
                                    AnchorPane.leftAnchor="0.0"
                                    AnchorPane.rightAnchor="0.0"
                                    AnchorPane.topAnchor="0.0">
                            <center>
                                <Label fx:id="statusLabel" wrapText="true">
                                    <padding>
                                        <Insets bottom="5.0" left="10.0"
                                                right="10.0" top="5.0"/>
                                    </padding>
                                </Label>
                            </center>
                            <left>
                                <HBox alignment="CENTER_LEFT" spacing="10.0">
                                    <children>
                                        <Label fx:id="progressLabel"
                                               cache="true" text="Progress:"/>
                                        <ProgressBar fx:id="progressBar"
                                                     cache="true"
                                                     progress="0.0"/>
                                        <Button mnemonicParsing="false"
                                                text="Button"/>
                                    </children>
                                    <padding>
                                        <Insets bottom="5.0" left="10.0"
                                                right="5.0" top="5.0"/>
                                    </padding>
                                </HBox>
                            </left>
                            <opaqueInsets>
                                <Insets/>
                            </opaqueInsets>
                        </BorderPane>
                    </children>
                </AnchorPane>
            </bottom>
        </BorderPane>
    </children>
</AnchorPane>
