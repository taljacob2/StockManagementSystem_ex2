<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@UserPane.css"
            xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="application.pane.resources.user.UserPane">
   <children>
      <BorderPane layoutX="57.0" layoutY="26.0" prefHeight="200.0"
                  prefWidth="200.0" AnchorPane.bottomAnchor="0.0"
                  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                  AnchorPane.topAnchor="0.0">
         <top>
            <AnchorPane BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="userNameLabel" alignment="CENTER"
                         prefHeight="42.0" prefWidth="79.0" styleClass="title"
                         AnchorPane.bottomAnchor="0.0"
                         AnchorPane.leftAnchor="0.0"
                         AnchorPane.rightAnchor="0.0"
                         AnchorPane.topAnchor="0.0"/>
               </children>
            </AnchorPane>
         </top>
         <center>
            <BorderPane fx:id="borderPaneToShowTheAnotherInnerPane"
                        prefHeight="200.0" prefWidth="200.0"
                        BorderPane.alignment="CENTER"/>
         </center>
         <left>
            <VBox alignment="TOP_CENTER" spacing="5.0"
                  BorderPane.alignment="CENTER">
               <children>
                  <Button fx:id="viewOwnsButton" mnemonicParsing="false"
                          text="View Owns"/>
                  <Button fx:id="makeAnOrderButton" mnemonicParsing="false"
                          text="Make an Order"/>
               </children>
               <opaqueInsets>
                  <Insets/>
               </opaqueInsets>
               <BorderPane.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
               </BorderPane.margin>
            </VBox>
         </left>
      </BorderPane>
   </children>
</AnchorPane>
